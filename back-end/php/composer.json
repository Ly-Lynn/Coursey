{
    "name": "your-username/your-project",
    "description": "A simple PHP project with Dotenv support",
    "require": {
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },

    "minimum-stability": "stable",
    "prefer-stable": true
}